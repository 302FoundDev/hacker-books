---
const links = [
  {
    "github": "https://github.com/302FoundDev",
    "twitter": "https://x.com/302founddev",
    "midu": "Gracias a @midudev por toda la ense;anza",
    "midusocial": "https://x.com/midudev",
    "rel": "noopener noreferer",
    "target": "_blank"
  }
]
---

<section>
  <div class="flex justify-between px-4 max-w-screen-xl mx-auto py-8">
    {
      links.map(({ github, twitter, midu, midusocial, rel, target }) => {

        return (
          <div>
            <span>Created by <a href={twitter} rel={rel} target={target} class="hover:underline hover:text-red-300 transition">@302FoundDev</a></span>
          </div>

          <div>
            <span>
             <a href={midusocial} rel={rel} target={target} class="hover:underline hover:text-red-300 transition">{midu}</a>
            </span>
          </div>

          <div>
            <span>
              <a href={github} rel={rel} target={target} class="hover:underline hover:text-red-300 transition">Github</a>
            </span>
          </div>
        )
      })
    }
  </div>
</section>

